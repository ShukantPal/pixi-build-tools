{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.29.1",
  "pnpmVersion": "4.14.4",
  "pnpmOptions": {
    "pnpmStore": "global",
    "strictPeerDependencies": false,
    "resolutionStrategy": "fast",
    "preventManualShrinkwrapChanges": true,
    "useWorkspaces": false
  },
  "nodeSupportedVersionRange": ">=10.0.0 <15.0.0",
  "suppressNodeLtsWarning": true,
  "ensureConsistentVersions": false,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 3,
  "repository": {
    "url": "https://github.com/SukantPal/pixi-build-tools",
    "defaultBranch": "release",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "projects": [
    {
      "packageName": "@pixi-build-tools/api-extractor-lerna-monorepo",
      "projectFolder": "packages/api-extractor-lerna-monorepo",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    {
      "packageName": "@pixi-build-tools/floss-rush-monorepo",
      "projectFolder": "packages/floss-rush-monorepo",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    {
      "packageName": "@pixi-build-tools/rollup-configurator",
      "projectFolder": "packages/rollup-configurator",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    {
      "packageName": "@pixi-build-tools/globals",
      "projectFolder": "packages/globals",
      "reviewCategory": "production",
      "shouldPublish": true
    }
  ]
}
